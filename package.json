{
  "name": "@compiled-fluent/config-sync",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/config",
    "packages/fs-loader",
    "packages/gh-loader"
  ],
  "scripts": {
    "predocs": "node ./scripts/prebuild.js",
    "prebuild": "node ./scripts/prebuild.js",
    "prewatch": "node ./scripts/prebuild.js",
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "docs": "typedoc --options typedoc.config.cjs"
  },
  "devDependencies": {
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "node-fetch": "^3.3.1"
  }
}
